/ {
	vbus2: vbus {
		compatible = "vbus-voltage-divider-adc";
		io-channels = <&adc1 3>;
		output-ohms = <49900>;
		full-ohms = <(330000 + 49900)>;

		/* Pin B14 is used to control VBUS Discharge for Port2 */
		discharge-gpios = <&gpiob 14 GPIO_ACTIVE_HIGH>;
	};

	ports {
		#address-cells = <1>;
		#size-cells = <0>;

		port1: usbc-port@1 {
			compatible = "usbc-port";
			label = "PORT1";
			reg = <1>;
			tcpc = <&ucpd2>;
			vbus = <&vbus2>;
		};
	};
};
